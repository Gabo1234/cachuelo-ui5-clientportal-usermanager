<mvc:View
    controllerName="clientportal.saasa.com.pe.usermanager.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
        <App id="app">
            <pages>
                <Page showHeader="false">
                    <content>
                    <Title id="idMainTitle" class="sapUiMediumMargin sapUiSmallMarginTopBottom" text="{i18n>title}"/>
                    <core:Fragment fragmentName="clientportal.saasa.com.pe.usermanager.view.fragment.UserFilter" type="XML"/>
                    <core:Fragment fragmentName="clientportal.saasa.com.pe.usermanager.view.fragment.UserBody" type="XML"/>
                    </content>
                </Page>
            </pages>
        </App>
</mvc:View>
